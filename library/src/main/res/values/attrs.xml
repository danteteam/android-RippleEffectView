<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleEffectView">
        <attr name="rippleColor" format="color"/>
        <attr name="rippleAnimationDuration" format="integer"/>
        <attr name="rippleStartAlpha" format="integer"/>
        <attr name="rippleClickDelay" format="integer"/>
        <attr name="rippleSoftReverse" format="boolean"/>
        <attr name="rippleEnable" format="boolean"/>
        <attr name="rippleEdgeType" format="enum">
            <enum name="soft" value="0"/>
            <enum name="strong" value="1"/>
        </attr>
    </declare-styleable>
</resources>